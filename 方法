import java.util.Scanner;
class test {
public static void main(String[] args){
    int a;
    int b;
    Scanner as=new Sanner(System.in);
    a=as.nextInt();
    b=as.nextInt();
    int sum=add(a,b);
    System.out.println(sum);

}
public static int add(int a,int b){
