import java.util.Scanner;
class Test{
	private int y;
	private int x;
	public Test() {}
	public void setx(int x) {
		this.x=x;
	}
	public int gety() {
		if(x<1) {
			 y=x;
		}else if(x>=1&&x<10) {
			 y=2*x-1;
		}else if(x>=10) {
			 y=3*10-11;
		}
		return y;
		
	}

}
public class Damo {

	public static void main(String[] args) {
		Test s=new Test();
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		s.setx(x);
		int add=s.gety();
		System.out.print(add);
	}

}
